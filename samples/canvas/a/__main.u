import a.canvas

class Handler {
    new(cv) = new()
}

print('before run')
canvas.run(
    title = 'canvas sample',
    init = def(cv) {
        print('init with %r' % [cv])
    },
    click = def(cv, x, y) {
        print('click %r, %r' % [x, y])
        cv.fill_rect(x, y, 10, 10)
    },
    mousemove = def(cv, x, y) {
        cv.fill_rect(x, y, 10, 10)
    },
)
